APP_NAME=user_backend
ENVIRONMENT=production
API_HOST=0.0.0.0
API_PORT=8010
LOG_LEVEL=info
DATABASE_URL=postgresql+asyncpg://<user>:<pass>@user_psql:5432/<db>
SYNC_DATABASE_URL=postgresql+psycopg://<user>:<pass>@user_psql:5432/<db>
JWT_SECRET=generate-strong-secret
JWT_ALG=HS256
JWT_ACCESS_TTL_SECONDS=3600
AUTH_COOKIE_NAME=botberi_session
REGISTRATION_CODE_TTL_SECONDS=900
REGISTRATION_CODE_COOLDOWN_SECONDS=60
PASSWORD_RESET_TTL_SECONDS=1800
PASSWORD_RESET_URL_TEMPLATE=https://app.botberi.com/reset-password?token={token}
VERIFICATION_EMAIL_SUBJECT=Complete your Botberi signup
PASSWORD_RESET_EMAIL_SUBJECT=Botberi password reset
EMAIL_SENDER=no-reply@botberi.com
FRONTEND_PROFILE_URL=https://app.botberi.com/profile
REDIS_URL=redis://:<password>@user_redis:6379/0
RABBITMQ_URL=amqp://<user>:<pass>@event_bus:5672/<vhost>
CELERY_DEFAULT_QUEUE=user_default
SHARED_PG_DSN=postgresql+asyncpg://<user>:<pass>@shared_psql:5432/<db>


